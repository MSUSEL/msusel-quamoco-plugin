	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.sonar</groupId>
				<artifactId>sonar-packaging-maven-plugin</artifactId>
				<configuration>
					<pluginKey>sonarquamoco</pluginKey>
					<pluginClass>net.siliconcode.sonar.quamoco.QuamocoPlugin</pluginClass>
					<pluginName>Sonar Quamoco Plugin</pluginName>
					<pluginDescription>Collects quality information about projects.</pluginDescription>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
